#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int x;
	    cin>>x;
	    if(n>x){
	        cout<<-1<<"\n";
	        continue;
	    }
	    cout<<x-n+1<<" ";
	    for(int i=1;  i<=n; i++){
	        if(i!=x-n+1){
	            cout<<i<<" ";
	            
	        }
	    }
	    cout<<"\n";
	}
	return 0;
}
